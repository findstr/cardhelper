@import "../common/ui.wxss";

#container::after {
  content: "";
  position: absolute;
  z-index: -1;
  background-color: inherit;
  width: 100%;
  height: 100%;
  right: -8px;
  bottom: -8px;
  opacity: 0.2;
  transform: scale(1, 1);
}

.text {
	font-size:22px;
	padding-left:10px;
}
